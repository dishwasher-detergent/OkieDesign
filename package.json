{
  "name": "okiedesign",
  "version": "1.1.2",
  "description": "Responsive CSS components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dishwasher-detergent/OkieDesign.git"
  },
  "author": "Kenneth Bass <kenny@kennethbass.com> (https://kennethbass.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dishwasher-detergent/OkieDesign/issues"
  },
  "homepage": "https://okie.design",
  "keywords": [
    "design-system",
    "tailwindcss",
    "components",
    "ui-library",
    "component",
    "framework",
    "tailwind",
    "postcss",
    "design",
    "css",
    "ui"
  ],
  "main": "index.js",
  "files": [
    "dist/*.js",
    "dist/colors/default_colors.js",
    "dist/{components}.css",
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "branches": [
      "master"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "cssnano": "^4.1.11",
    "postcss": "^8.4.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.6",
    "prejss-cli": "^0.3.3",
    "tailwindcss": "^2.2.19"
  },
  "scripts": {
    "precomponents": "node-sass src/components/ -o dist/components/css/",
    "components": "type dist\\components\\css\\*.css > dist\\components.css",
    "postcomponents": "prejss-cli dist/components.css --format commonjs",
    "build": "npm run components"
  }
}
